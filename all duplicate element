

class Solution
{
public:
    vector<int> findDuplicates(vector<int> &nums)
    {
        unordered_set<int> s;
        vector<int> duplicate;
        for (int i = 0; i < nums.size(); i++)
        {
            if (s.find(nums[i]) == s.end())
                s.insert(nums[i]);
            else
                duplicate.push_back(nums[i]);
        }
        return duplicate;
    }
};
